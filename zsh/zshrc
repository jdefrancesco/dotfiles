# This will clear ALL ideas (only relevant if "idea" program is installed)
clearideas() {
    for i in {1..$1}; do idea solve $i; done
}

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

# Forgit GIT aliases
source ~/.forgit/forgit.plugin.zsh

# Function:    backup
# Description: Backup a file. Appends date and .bak extension.
function backup() {
    newname=$1.$(date +%Y-%m-%d.%H%M.bak);
    mv $1 $newname;
    echo "Backed up $1 to ${newname}";
    cp -p ${newname} $1;
}


